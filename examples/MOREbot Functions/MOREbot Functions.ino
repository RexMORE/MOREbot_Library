// MOREbot_Library - Version: Latest #include <MOREbot.h>#define LM 4#define RM 1#define trig 3#define echo 4MOREbot bot(LM, RM, trig, echo);void setup() {  bot.setup();  Serial.begin(9600);}void loop() {  float dist = bot.readDistance();  Serial.println(dist);  delay(20);  bot.forward(100);  delay(100);  bot.right(50);  delay(100);  bot.left(50);  delay(100);  bot.backward(100);  delay(100);  bot.stop();  delay(80);}